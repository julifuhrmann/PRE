<div class="container">
  <div class="row">
    <%= simple_form_for(party) do |f| %>
      <%= f.input :photo, as: :file %>
      <%= f.input :name %>
      <%= f.input :date, as: :date %>
      <%= f.input :status %>
      <%= f.input :description %>
      <%= f.input :user %>
      <%= f.input :address %>
      <%= f.submit %>
    <% end %>
  </div>
</div>
